<BizItemSchema itemType="BizItemSchemas" targetItemType="Property">
    <System>
        <Property name="id"           caption="System ID"     type="ObjectId"/>
        <Property name="itemType"     caption="Item Type"     type="String" indexed="false" unique="false" constrints="NotNull,Size_1_100"/>
        <Property name="trackingId"   caption="TrackingID"    type="String" indexed="true"  unique="true" autogenerated="true" constrints="NotNull,Size_1_100"/>
        <Property name="createdDate"  caption="Created Date"  type="Date"   indexed="true"  unique="false" constrints="NotNull"/>
        <Property name="modifiedDate" caption="Modified Date" type="Date"   indexed="true"  unique="false" constrints="NotNull"/>
    </System>

    <Content>
         <Property name="userTID"       caption="User TID"      type="String" constrints="NotNull,Size_1_100"/>
         <Property name="buildingType"  caption="Building Type" type="Enum"   enumName="EnPropertyBuildType" constrints="Enum"/>
         <Property name="condition"     caption="Condition"     type="Enum"   enumName="EnPropertyCondition"    constrints="Enum"/>
         <Property name="rooms"         caption="Rooms"         type="Integer"   constrints="Min_1"/>
         <Property name="bathRooms"     caption="Bathrooms"     type="Integer"   constrints="Min_0"/>
         <Property name="balconies"     caption="Balconies"     type="Integer"   constrints="Min_0"/>
         <Property name="thumbnail"     caption="Thumbnail"     type="ByteArray" constrints="MaxSizeMB_1"/>
         <Property name="parking"       caption="Parking"       type="Boolean"   constrints=""/>
    </Content>
</BizItemSchema>