<BizItemSchema itemType="BizItemSchemas" targetItemType="PropertyListing">
    <System>
        <Property name="id"           caption="System ID"     type="ObjectId"/>
        <Property name="itemType"     caption="Item Type"     type="String" indexed="false" unique="false" constrints="NotNull,Size_1_100"/>
        <Property name="trackingId"   caption="TrackingID"    type="String" indexed="true"  unique="true" autogenerated="true" constrints="NotNull,Size_1_100"/>
        <Property name="createdDate"  caption="Created Date"  type="Date"   indexed="true"  unique="false" constrints="NotNull"/>
        <Property name="modifiedDate" caption="Modified Date" type="Date"   indexed="true"  unique="false" constrints="NotNull"/>
    </System>

    <Content>
         <Property name="userTID"       caption="User ID"       type="String"  constrints="NotNull,Size_1_100"/>
         <Property name="propertyTID"   caption="Property ID"   type="String"  constrints="NotNull,Size_1_100"/>
         <Property name="listed"        caption="Listed"        type="Boolean" constrints=""/>
         <Property name="price"         caption="Price"         type="Long"    constrints="Min_0"/>
         <Property name="views"         caption="Listed"        type="Integer" constrints="Min_0"/>
         <Property name="checkInDate"   caption="CheckIn Date"  type="Date"    constrints="NotNull,FutureDate"/>
         <Property name="checkOutDate"  caption="CheckOut Date" type="Date"    constrints="NotNull,FutureDate"/>
    </Content>
</BizItemSchema>